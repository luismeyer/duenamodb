{
  "name": "duenamodb",
  "version": "1.2.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "keywords": [
    "DynamoDB",
    "DDB",
    "AWS DynamoDB",
    "DynamoDB Client",
    "Amazon DynamoDB"
  ],
  "scripts": {
    "watch": "tsc -w",
    "build": "tsc",
    "pretest": "ts-node scripts/setup-db",
    "posttest": "ts-node scripts/stop-db",
    "test": "ava",
    "prettier": "prettier --check .",
    "prettier:write": "prettier --write .",
    "bump": "ts-node scripts/update-version"
  },
  "dependencies": {
    "aws-sdk": "^2.1120.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.16",
    "@types/prettier": "^2.4.4",
    "ava": "^4.1.0",
    "dynamodb-local": "^0.0.31",
    "find-process": "^1.4.7",
    "prettier": "2.6.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.5.5"
  },
  "ava": {
    "timeout": "1m",
    "files": [
      "test/**/*",
      "!test/helper/**/*"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "test": {
    "dbPort": 8000,
    "tablename": "test-table",
    "indexname": "test-gsi"
  }
}
