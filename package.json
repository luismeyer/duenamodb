{
	"name": "duenamodb",
	"version": "1.4.0",
	"keywords": [
		"DynamoDB",
		"DDB",
		"AWS DynamoDB",
		"DynamoDB Client",
		"Amazon DynamoDB"
	],
	"license": "MIT",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"files": [
		"lib"
	],
	"scripts": {
		"build": "tsc",
		"bump": "ts-node scripts/update-version",
		"format": "biome format .",
		"format:write": "biome format --write .",
		"test": "ava",
		"posttest": "ts-node scripts/stop-db",
		"watch": "tsc -w"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.306.0",
		"@aws-sdk/util-dynamodb": "^3.306.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/node": "^17.0.16",
		"ava": "^6.1.0",
		"find-process": "^1.4.7",
		"ts-node": "^10.7.0",
		"typescript": "^4.5.5"
	}
}
